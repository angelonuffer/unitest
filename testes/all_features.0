unitest = ../c√≥digo/0

unitest.describe({
  name: "All Features Demo"
  tests: [
    unitest.describe({
      name: "Basic Equality"
      tests: [
        unitest.equals([ 42 42 ])
        unitest.equals([ "test" "test" ])
        unitest.equals([ 0 0 ])
      ]
    })
    unitest.describe({
      name: "List Comparisons"
      tests: [
        unitest.lists_equal([ [ ] [ ] ])
